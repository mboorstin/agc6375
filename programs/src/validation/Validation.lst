005302,000026:                                                                                         	#-------------------------------------------------------------------------
005303,000027:                                                                                         	# Display the error code.  What this does is to clear the entire display,
005304,000028:                                                                                         	# except for MODE (which contains the error code), and then lights the
005305,000029:                                                                                         	# OPR ERR lamp and waits for the ENTR key to be hit.  Then it unlights
005306,000030:                                                                                         	# OPR ERR and continues.
005307,000031: 
005308,000032:    7240                        ERRORDSP                                                 	#
005309,000033:                                                                                         	# Save return address in TEMPK
005310,000034:    7240           00006                          EXTEND                                 	#
005311,000035:    7241           22065                          QXCH     TEMPK                         	#
005312,000036: 
005313,000037:                                                                                         	# Blank out all DSKY fields except MODE.  This means
005314,000038:                                                                                         	# writing out 50000, 44000, ..., 04000.
005315,000039:    7242           37404                          CA       O50000                        	#
005316,000040:    7243           54001                          TS       L                             	# L is the value we want to write to ch 10.
005317,000041:    7244           37430                          CA       TEN                           	# A is the loop counter.
005318,000042:    7245           56001        ERRORLOO          XCH      L                             	#
005319,000043:                                                                                         	# TC	IODELAY
005320,000044:    7246           00006                          EXTEND                                 	#
005321,000045:    7247           01010                          WRITE    CH10                          	#
005322,000046:    7250           67411                          AD       O-4000                        	#
005323,000047:    7251           56001                          XCH      L                             	#
005324,000048:    7252           10000                          CCS      A                             	#
005325,000049:    7253           17245                          TCF      ERRORLOO                      	#
005326,000050: 
005327,000051:                                                                                         	# Output ERRNUM in MODE.
005328,000052:    7254           37403                          CA       O54000                        	#
005329,000053:    7255           54063                          TS       TEMPI                         	#
005330,000054:    7256           30061                          CA       ERRNUM                        	#
005331,000055:    7257           54064                          TS       TEMPJ                         	#
005332,000056:                                                                                         	# TC	IODELAY
005333,000057:    7260           07322                          TC       DIGPAIR                       	#
005334,000058: 
005335,000059:                                                                                         	# Output ERRSUB in NOUN.
005336,000060:    7261           37405                          CA       O44000                        	#
005337,000061:    7262           54063                          TS       TEMPI                         	#
005338,000062:    7263           30062                          CA       ERRSUB                        	#
005339,000063:    7264           54064                          TS       TEMPJ                         	#
005340,000064:                                                                                         	# TC	IODELAY
005341,000065:    7265           07322                          TC       DIGPAIR                       	#
005342,000066: 
005343,000067:                                                                                         	# Turn on the OPR ERR lamp.
005344,000068:    7266           37434                          CA       BIT7                          	#
005345,000069:                                                                                         	# TC	IODELAY
005346,000070:    7267           00006                          EXTEND                                 	#
005347,000071:    7270           01011                          WRITE    CH11                          	#
005348,000072: 
005349,000073:                                                                                         	# Wait for PRO key to be pressed.  Please recall that
005350,000074:                                                                                         	# the PRO key is a low-polarity signal:  0 when pressed,
005351,000075:                                                                                         	# 1 when released.
005352,000076:    7271           00006        PRONWAIT          EXTEND                                 	#
005353,000077:    7272           00032                          READ     CH32                          	#
005354,000078:    7273           77435                          MASK     BIT14                         	#
005355,000079:    7274           10000                          CCS      A                             	#
005356,000080:    7275           17271                          TCF      PRONWAIT                      	#
005357,000081:    7276                        PROPRESS                                                 	#
005358,000082: 
005359,000083:                                                                                         	# Turn off the OPR ERR lamp.
005360,000084:    7276           30007                          CA       ZEROES                        	#
005361,000085:                                                                                         	# TC	IODELAY
005362,000086:    7277           00006                          EXTEND                                 	#
005363,000087:    7300           01011                          WRITE    CH11                          	#
005364,000088: 
005365,000089:                                                                                         	# Wait for PRO key to be released.
005366,000090:    7301           00006        PROFWAIT          EXTEND                                 	#
005367,000091:    7302           00032                          READ     CH32                          	#
005368,000092:    7303           77435                          MASK     BIT14                         	#
005369,000093:    7304           10000                          CCS      A                             	#
005370,000094:    7305           17307                          TCF      +2                            	#
005371,000095:    7306           17301                          TCF      PROFWAIT                      	#
005372,000096: 
005373,000097:                                                                                         	# Restore the return address.  (Could actually just
005374,000098:                                                                                         	# "TC TEMPK" here.)
005375,000099:    7307           00006                          EXTEND                                 	#
005376,000100:    7310           22065                          QXCH     TEMPK                         	#
005377,000101:    7311           00002                          RETURN                                 	#
005378,000102: 
005379,000103:                                                                                         	#-------------------------------------------------------------------------
005380,000104:                                                                                         	# Delay a little while.  This is a workaround for some characteristics
005381,000105:                                                                                         	# (as of 07/17/04, anyhow) of Webb's AGC-DSKY interaction.  No registers
005382,000106:                                                                                         	# are affected, except that any overflow in the accumulator will be lost.
005383,000107: 
005384,000108:    7312           00764        IODELAYT          DEC      500        B-14               	#
005385,000109: 
005386,000110:    7313           54064        IODELAY           TS       TEMPJ                         	# Save accumulator.
005387,000111:    7314           17315                          NOOP                                   	#
005388,000112:    7315           37312                          CA       IODELAYT                      	#
005389,000113:    7316           10000                          CCS      A                             	#
005390,000114:    7317           17316                          TCF      -1                            	#
005391,000115:    7320           30064                          CA       TEMPJ                         	# Restore accumulator.
005392,000116:    7321           00002                          RETURN                                 	#
005393,000117: 
005394,000118:                                                                                         	#-------------------------------------------------------------------------
005395,000119:                                                                                         	# Display a two-digit octal code.  On entry, TEMPI should contain a
005396,000120:                                                                                         	# code corresponding to the digit pair, as follows:
005397,000121:                                                                                         	#	54000	MODE
005398,000122:                                                                                         	#	50000	VERB
005399,000123:                                                                                         	#	44000	NOUN
005400,000124:                                                                                         	#	40000	Not used/1st digit of R1
005401,000125:                                                                                         	#	34000	2nd/3rd digit of R1
005402,000126:                                                                                         	#	30000	4th/5th digit of R1
005403,000127:                                                                                         	#	24000	1st/2nd digit of R2
005404,000128:                                                                                         	#	20000	3rd/4th digit of R2
005405,000129:                                                                                         	#	14000	5th digit of R2/1st digit of R3
005406,000130:                                                                                         	#	10000	2nd/3rd digit of R3
005407,000131:                                                                                         	#	04000	4th/5th digit of R3
005408,000132:                                                                                         	# On entry also, TEMPJ contains the number we want displayed.
005409,000133: 
005410,000134:    7322                        DIGPAIR                                                  	# Shift the error code down by 3 places, so as to get
005411,000135:                                                                                         	# the first octal digit.
005412,000136:    7322           30064                          CA       TEMPJ                         	#
005413,000137:    7323           54020                          TS       CYR                           	#
005414,000138:    7324           30020                          CA       CYR                           	#
005415,000139:    7325           30020                          CA       CYR                           	#
005416,000140:    7326           30020                          CA       CYR                           	#
005417,000141:    7327           77431                          MASK     SEVEN                         	#
005418,000142:                                                                                         	# Convert to a pattern for output.
005419,000143:    7330           50000                          INDEX    A                             	#
005420,000144:    7331           37362                          CA       DSP0                          	#
005421,000145:    7332           54022                          TS       CYL                           	#
005422,000146:    7333           30022                          CA       CYL                           	#
005423,000147:    7334           30022                          CA       CYL                           	#
005424,000148:    7335           30022                          CA       CYL                           	#
005425,000149:    7336           30022                          CA       CYL                           	#
005426,000150:    7337           30022                          CA       CYL                           	#
005427,000151:    7340           60063                          AD       TEMPI                         	#
005428,000152:    7341           54063                          TS       TEMPI                         	#
005429,000153:                                                                                         	# Now do the same for the second octal digit, except that
005430,000154:                                                                                         	# all the shifting isn't neeeded.
005431,000155:    7342           30064                          CA       TEMPJ                         	#
005432,000156:    7343           77431                          MASK     SEVEN                         	#
005433,000157:    7344           50000                          INDEX    A                             	#
005434,000158:    7345           37362                          CA       DSP0                          	#
005435,000159:    7346           60063                          AD       TEMPI                         	#
005436,000160:    7347           54063                          TS       TEMPI                         	#
005437,000161:                                                                                         	# Output it!
005438,000162:    7350           00006                          EXTEND                                 	#
005439,000163:    7351           01010                          WRITE    CH10                          	#
005440,000164: 
005441,000165:    7352           00002                          RETURN                                 	#
005442,000166: 
